<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="SimpleReport" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
    <parameter name="REPORT_ID" class="java.lang.String"/>
    <title>
        <band height="79" splitType="Stretch">
            <staticText>
                <reportElement x="180" y="10" width="200" height="30"/>
                <textElement textAlignment="Center">
                    <font size="20"/>
                </textElement>
                <text><![CDATA[Generated Report]]></text>
            </staticText>
            <textField>
                <reportElement x="180" y="40" width="200" height="30"/>
                <textElement textAlignment="Center">
                    <font size="14"/>
                </textElement>
                <textFieldExpression><![CDATA["Report ID: " + $P{REPORT_ID}]]></textFieldExpression>
            </textField>
        </band>
    </title>
</jasperReport>
